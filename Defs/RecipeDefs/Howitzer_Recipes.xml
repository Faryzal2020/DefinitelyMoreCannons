<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
    <defName>Make_Sig33Materials</defName>
    <label>make 15cm Howitzer materials</label>
    <description>Make materials needed to build a 15cm Howitzer</description>
    <jobString>Making cannon materials.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>1000</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>ComponentIndustrial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SigHowitzerMaterials>1</SigHowitzerMaterials>
    </products>
		<skillRequirements>
			<Crafting>3</Crafting>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>CNCmachine</li>
    </recipeUsers>
  </RecipeDef>

